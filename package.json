{
  "name": "@appril/ui",
  "version": "0.2.3",
  "description": "Various UI Plugins for Appril Apps",
  "author": "Slee Woo",
  "license": "MIT",
  "engines": {
    "node": "20.10",
    "npm": "10.2"
  },
  "files": [
    "pkg/*"
  ],
  "// ✨ seems exports.types not fully supported by all editors yet?": "",
  "types": "./pkg/lib/index.d.ts",
  "exports": {
    ".": {
      "default": "./pkg/index.js",
      "types": "./pkg/lib/index.d.ts"
    },
    "./style.css": {
      "default": "./pkg/style.css"
    }
  },
  "scripts": {
    "// ✨ build": "",
    "build": "cd lib; vite build",
    "// ✨ publish": "",
    "prepub": "npm run build",
    "pub": "npm pub",
    "postpub": "mkdir -p var/pub; npm pack --pack-destination var/pub"
  },
  "peerDependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1",
    "bootstrap": "^5.3.2",
    "floating-vue": "^2.0.0-beta.24",
    "vue": "^3.4.3"
  },
  "devDependencies": {
    "@appril/dev": "^0.2.3",
    "@appril/scripts": "^0.2.3",
    "@babel/types": "^7.23.6",
    "@types/bootstrap": "^5.2.10",
    "@types/node": "^20.10.6",
    "@vitejs/plugin-vue": "^5.0.2",
    "release-it": "^17.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-dts": "^3.7.0",
    "vue-tsc": "^1.8.27"
  }
}
